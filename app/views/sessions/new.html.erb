<form action="/signin" method="POST">
    <input type="hidden"
             name="authenticity_token"
             	value="<%= form_authenticity_token %>">
	<input type="text" placeholder="Username" name=session[user_name]>
	<input type="password" placeholder="Password" name=session[password]>
	<input type="submit" value="Log in">
</form>